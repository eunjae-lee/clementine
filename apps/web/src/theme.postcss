:root {
	/* =~= Theme Properties =~= */
	--theme-font-family-base: system-ui;
	--theme-font-family-heading: system-ui;
	--theme-font-color-base: 0 0 0;
	--theme-font-color-dark: 255 255 255;
	--theme-rounded-base: 6px;
	--theme-rounded-container: 6px;
	--theme-border-base: 1px;
	/* =~= Theme On-X Colors =~= */
	--on-primary: 255 255 255;
	--on-secondary: var(--color-secondary-50);
	--on-tertiary: var(--color-tertiary-50);
	--on-success: var(--color-secondary-900);
	--on-warning: 0 0 0;
	--on-error: var(--color-error-50);
	--on-surface: 0 0 0;
	/* =~= Theme Colors  =~= */
	/* primary | #DE4500 */
	--color-primary-50: 250 227 217; /* ⬅ #fae3d9 */
	--color-primary-100: 248 218 204; /* ⬅ #f8dacc */
	--color-primary-200: 247 209 191; /* ⬅ #f7d1bf */
	--color-primary-300: 242 181 153; /* ⬅ #f2b599 */
	--color-primary-400: 232 125 77; /* ⬅ #e87d4d */
	--color-primary-500: 222 69 0; /* ⬅ #DE4500 */
	--color-primary-600: 200 62 0; /* ⬅ #c83e00 */
	--color-primary-700: 167 52 0; /* ⬅ #a73400 */
	--color-primary-800: 133 41 0; /* ⬅ #852900 */
	--color-primary-900: 109 34 0; /* ⬅ #6d2200 */
	/* secondary | #0b6b34 */
	--color-secondary-50: 218 233 225; /* ⬅ #dae9e1 */
	--color-secondary-100: 206 225 214; /* ⬅ #cee1d6 */
	--color-secondary-200: 194 218 204; /* ⬅ #c2dacc */
	--color-secondary-300: 157 196 174; /* ⬅ #9dc4ae */
	--color-secondary-400: 84 151 113; /* ⬅ #549771 */
	--color-secondary-500: 11 107 52; /* ⬅ #0b6b34 */
	--color-secondary-600: 10 96 47; /* ⬅ #0a602f */
	--color-secondary-700: 8 80 39; /* ⬅ #085027 */
	--color-secondary-800: 7 64 31; /* ⬅ #07401f */
	--color-secondary-900: 5 52 25; /* ⬅ #053419 */
	/* tertiary | #006e99 */
	--color-tertiary-50: 217 233 240; /* ⬅ #d9e9f0 */
	--color-tertiary-100: 204 226 235; /* ⬅ #cce2eb */
	--color-tertiary-200: 191 219 230; /* ⬅ #bfdbe6 */
	--color-tertiary-300: 153 197 214; /* ⬅ #99c5d6 */
	--color-tertiary-400: 77 154 184; /* ⬅ #4d9ab8 */
	--color-tertiary-500: 0 110 153; /* ⬅ #006e99 */
	--color-tertiary-600: 0 99 138; /* ⬅ #00638a */
